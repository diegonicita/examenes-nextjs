{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNEXT",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "server-actions/helpers/mysqldb.js",
    "app/(pages)/products-server/page.tsx",
    "server-actions/products/insertProductAction.js",
    "server-actions/products/createProduct.js",
    "server-actions/products/displayProducts.js",
    "app/(pages)/home/components/consults/consults.tsx",
    "app/(pages)/home/components/consults/submit.tsx"
, "app/(pages)/questions-algolia/components/searchbar.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}